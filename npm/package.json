{
  "name": "rhx",
  "version": "0.2.4",
  "description": "Native npx launcher for the rhx CLI",
  "license": "MIT",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "rhx": "bin/rhx.cjs"
  },
  "files": [
    "bin",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finlayi/robinhood-cli.git",
    "directory": "npm"
  },
  "keywords": [
    "robinhood",
    "cli",
    "python",
    "npx",
    "trading"
  ],
  "optionalDependencies": {
    "rhx-darwin-arm64": "0.2.4",
    "rhx-linux-x64": "0.2.4",
    "rhx-win32-x64": "0.2.4"
  },
  "scripts": {
    "sync:versions": "node scripts/sync-platform-versions.cjs",
    "test:versions": "node scripts/sync-platform-versions.cjs --check",
    "test": "npm run test:versions && node --test test/*.test.cjs"
  }
}
